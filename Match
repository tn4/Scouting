    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        System.out.println("auton robot");
        if(button_one)
        {
            jButton1.setForeground(Color.RED);
            button_one = false;
        }
        else
        {
            jButton1.setForeground(Color.BLACK);
            button_one = true;
        }
    }                                        

