    private void jButton10ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        System.out.println("RECORDED");

        textfield_one = Integer.parseInt(jTextField1.getText());
        textfield_two = Integer.parseInt(jTextField2.getText());
        textfield_three = Integer.parseInt(jTextField3.getText());
        textfield_four = Integer.parseInt(jTextField4.getText());
        textfield_five = Integer.parseInt(jTextField5.getText());
        textfield_six = Integer.parseInt(jTextField6.getText());
        textfield_seven = Integer.parseInt(jTextField7.getText());
        textfield_eight = Integer.parseInt(jTextField8.getText());
        textfield_nine = Integer.parseInt(jTextField9.getText());
        textfield_ten = Integer.parseInt(jTextField10.getText());
        textfield_eleven = Integer.parseInt(jTextField11.getText());
        textfield_twelve = Integer.parseInt(jTextField12.getText());

        DataMake();
    }                                         
    public void DataMake()
    {
        FileWriter f = null;
        try
        {
            f = new FileWriter("URL", true);
            f.write(textfield_one + " \n" );
            f.write(textfield_two + " \n" );
            f.write(textfield_three + " \n" );
            f.write(textfield_four + " \n" );
            f.write(textfield_five + " \n" );
            f.write(textfield_six + " \n" );
            f.write(textfield_seven + " \n" );
            f.write(textfield_eight + " \n" );
            f.write(textfield_nine + " \n" );
            f.write(textfield_ten + " \n" );
            f.write(textfield_eleven + " \n" );
            f.write(textfield_twelve + " \n" );


        }
        catch (IOException ex)
        {

            Logger.getLogger(Match.class.getName()).log(Level.SEVERE, null, ex);
        }
        finally
        {
            try
            {
                f.close();
            } catch (IOException ex)
            {
                Logger.getLogger(Match.class.getName()).log(Level.SEVERE, null, ex);
            }
        }
        
        
    }
